<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox xmlns = "http://javafx.com/javafx"
      xmlns:fx = "http://javafx.com/fxml"
      fx:controller="com.example.pcfx.LoginController"
      alignment="CENTER" spacing="10">

    <Text text="login" style="-fx-font-size: 24px"/>
    <TextField fx:id="emailField" promptText="Email"/>
    <PasswordField fx:id="passwordField" promptText="Password"/>

    <Button text="Login" onAction="#handleLoginButtonAction"/>

    <Text fx:id="loginMessage" fill="red"/>

    <Button text="Crear Cuenta" onAction="#handleLoginButtonAction"/>


</VBox>
